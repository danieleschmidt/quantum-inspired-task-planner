{
  "dashboard": {
    "title": "Quantum Task Planner - Production Monitoring",
    "version": "1.0.0",
    "refresh": "5s",
    "panels": [
      {
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "metric": "quantum_planner_uptime",
            "legend": "Uptime"
          },
          {
            "metric": "quantum_planner_requests_total",
            "legend": "Total Requests"
          },
          {
            "metric": "quantum_planner_active_users",
            "legend": "Active Users"
          }
        ]
      },
      {
        "title": "Optimization Performance",
        "type": "graph",
        "targets": [
          {
            "metric": "quantum_planner_optimization_latency_p95",
            "legend": "P95 Latency"
          },
          {
            "metric": "quantum_planner_optimization_latency_median",
            "legend": "Median Latency"
          }
        ],
        "yAxis": {
          "unit": "seconds",
          "min": 0
        },
        "alert": {
          "condition": "p95 > 10",
          "severity": "warning"
        }
      },
      {
        "title": "Cache Performance",
        "type": "graph",
        "targets": [
          {
            "metric": "quantum_planner_cache_hit_ratio",
            "legend": "Hit Ratio"
          },
          {
            "metric": "quantum_planner_cache_l1_hits",
            "legend": "L1 Hits"
          },
          {
            "metric": "quantum_planner_cache_l2_hits",
            "legend": "L2 Hits"
          },
          {
            "metric": "quantum_planner_cache_l3_hits",
            "legend": "L3 Hits"
          }
        ],
        "yAxis": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "alert": {
          "condition": "hit_ratio < 70",
          "severity": "warning"
        }
      },
      {
        "title": "Concurrent Processing",
        "type": "graph",
        "targets": [
          {
            "metric": "quantum_planner_active_jobs",
            "legend": "Active Jobs"
          },
          {
            "metric": "quantum_planner_queue_size_high_priority",
            "legend": "High Priority Queue"
          },
          {
            "metric": "quantum_planner_queue_size_normal",
            "legend": "Normal Priority Queue"
          },
          {
            "metric": "quantum_planner_queue_size_batch",
            "legend": "Batch Queue"
          }
        ],
        "yAxis": {
          "unit": "count",
          "min": 0
        }
      },
      {
        "title": "Security Metrics",
        "type": "graph",
        "targets": [
          {
            "metric": "quantum_planner_auth_failures",
            "legend": "Authentication Failures"
          },
          {
            "metric": "quantum_planner_rate_limit_violations",
            "legend": "Rate Limit Violations"
          },
          {
            "metric": "quantum_planner_security_events",
            "legend": "Security Events"
          }
        ],
        "yAxis": {
          "unit": "events/min",
          "min": 0
        },
        "alert": {
          "condition": "security_events > 10",
          "severity": "critical"
        }
      },
      {
        "title": "Global Compliance",
        "type": "stat",
        "targets": [
          {
            "metric": "quantum_planner_gdpr_consent_rate",
            "legend": "GDPR Consent Rate"
          },
          {
            "metric": "quantum_planner_ccpa_compliance_rate",
            "legend": "CCPA Compliance"
          },
          {
            "metric": "quantum_planner_pdpa_compliance_rate",
            "legend": "PDPA Compliance"
          }
        ],
        "thresholds": [
          {
            "value": 95,
            "color": "green"
          },
          {
            "value": 90,
            "color": "yellow"
          },
          {
            "value": 0,
            "color": "red"
          }
        ],
        "alert": {
          "condition": "consent_rate < 95",
          "severity": "critical"
        }
      },
      {
        "title": "Regional Distribution",
        "type": "pie",
        "targets": [
          {
            "metric": "quantum_planner_requests_by_region",
            "legend": "Requests by Region"
          }
        ]
      },
      {
        "title": "Quantum Backend Status",
        "type": "table",
        "targets": [
          {
            "metric": "quantum_planner_backend_availability",
            "legend": "Backend Availability"
          },
          {
            "metric": "quantum_planner_backend_queue_time",
            "legend": "Backend Queue Time"
          }
        ]
      },
      {
        "title": "Research Algorithm Performance",
        "type": "graph",
        "targets": [
          {
            "metric": "quantum_planner_qaoa_success_rate",
            "legend": "QAOA Success Rate"
          },
          {
            "metric": "quantum_planner_vqe_convergence_rate",
            "legend": "VQE Convergence"
          },
          {
            "metric": "quantum_planner_hybrid_efficiency",
            "legend": "Hybrid Efficiency"
          },
          {
            "metric": "quantum_planner_annealing_success_rate",
            "legend": "Annealing Success"
          }
        ],
        "yAxis": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      {
        "title": "Data Processing Compliance",
        "type": "graph",
        "targets": [
          {
            "metric": "quantum_planner_data_encryption_rate",
            "legend": "Data Encryption Rate"
          },
          {
            "metric": "quantum_planner_data_residency_compliance",
            "legend": "Data Residency Compliance"
          },
          {
            "metric": "quantum_planner_user_rights_requests",
            "legend": "User Rights Requests"
          }
        ],
        "yAxis": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    ],
    "alerts": [
      {
        "name": "High Optimization Latency",
        "condition": "quantum_planner_optimization_latency_p95 > 10",
        "severity": "warning",
        "notification": [
          "email",
          "slack"
        ]
      },
      {
        "name": "Low Cache Hit Ratio",
        "condition": "quantum_planner_cache_hit_ratio < 70",
        "severity": "warning",
        "notification": [
          "slack"
        ]
      },
      {
        "name": "Security Violation",
        "condition": "quantum_planner_security_events > 10",
        "severity": "critical",
        "notification": [
          "email",
          "slack",
          "pagerduty"
        ]
      },
      {
        "name": "Compliance Violation",
        "condition": "quantum_planner_gdpr_consent_rate < 95",
        "severity": "critical",
        "notification": [
          "email",
          "slack",
          "pagerduty"
        ]
      },
      {
        "name": "Quantum Backend Down",
        "condition": "quantum_planner_backend_availability < 80",
        "severity": "warning",
        "notification": [
          "slack"
        ]
      },
      {
        "name": "High Error Rate",
        "condition": "quantum_planner_error_rate > 5",
        "severity": "critical",
        "notification": [
          "email",
          "slack",
          "pagerduty"
        ]
      }
    ],
    "variables": [
      {
        "name": "region",
        "type": "query",
        "query": "label_values(quantum_planner_requests_total, region)",
        "current": {
          "value": "all",
          "text": "All Regions"
        }
      },
      {
        "name": "time_range",
        "type": "interval",
        "options": [
          "5m",
          "15m",
          "1h",
          "6h",
          "24h",
          "7d"
        ],
        "current": {
          "value": "1h",
          "text": "Last 1 Hour"
        }
      }
    ]
  },
  "metadata": {
    "created_at": "2024-01-01T00:00:00Z",
    "version": "1.0.0",
    "author": "Quantum Task Planner Team",
    "description": "Comprehensive monitoring dashboard for production Quantum Task Planner deployment"
  }
}