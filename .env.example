# Quantum Backend Credentials
# D-Wave Configuration
DWAVE_TOKEN=your_dwave_token_here
DWAVE_SOLVER=Advantage_system6.1

# Azure Quantum Configuration  
AZURE_QUANTUM_SUBSCRIPTION_ID=your_subscription_id
AZURE_QUANTUM_RESOURCE_GROUP=your_resource_group
AZURE_QUANTUM_WORKSPACE=your_workspace_name
AZURE_QUANTUM_LOCATION=westus

# IBM Quantum Configuration
IBM_QUANTUM_TOKEN=your_ibm_token_here
IBM_QUANTUM_BACKEND=ibmq_qasm_simulator

# Classical Solver Configuration
CLASSICAL_TIMEOUT=300
QUANTUM_TIMEOUT=60
MAX_PROBLEM_SIZE=1000

# Performance and Caching
ENABLE_RESULT_CACHING=true
CACHE_DIRECTORY=./quantum_results_cache
MAX_CACHE_SIZE_MB=1024

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json
ENABLE_PERFORMANCE_LOGGING=true

# Testing Configuration
ENABLE_QUANTUM_TESTS=false
TEST_BACKEND=simulator
BENCHMARK_MODE=false

# Development Configuration
DEBUG=false
DEVELOPMENT_MODE=true
ENABLE_PROFILING=false
EOF < /dev/null
